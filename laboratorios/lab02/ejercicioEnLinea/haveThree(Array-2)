public boolean haveThree(int[] nums) {
  int tres = 0;
  boolean haveTres = false;
  if(nums.length < 5){return haveTres;}
  for(int i = 0; i < nums.length; i++){
    if(nums[i] == 3){
      if(i == 0 && nums[i + 1] != 3){tres = tres + 1;}
      else if(nums[i - 1] != 3){tres = tres + 1;}
    }
  }
  if(tres == 3){ haveTres = true;}
  return haveTres;
}
