public int[] evenOdd(int[] nums) {
  int [] oddEven = new int[nums.length];
  int  j = 0;
  for(int k = 0; k <= 1; k++){
    for(int i = 0; i < nums.length; i++){
      if(k == 0){
        if(nums[i] % 2 == 0){
          oddEven[j] = nums[i];
          j++;
        }
      }
      else{
        if(nums[i] % 2 != 0){
          oddEven[j] = nums[i];
          j++;
        }
      }
    }
  }
  return oddEven;
}
