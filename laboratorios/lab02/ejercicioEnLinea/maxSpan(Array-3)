public int maxSpan(int[] nums) {
  int max = 0, l = 0, r = 0, span = 0;
  for(int k = 0; k < nums.length; k++){
    for(int i = k; i  < nums.length; i++){
      if(nums[k] == nums[i]){ 
        for(int j = k; j <= i; j++){
          span = span + 1;
        }
        if(span > max){
          max = span;
        }
      }
      span = 0;
    }
  }
  return max;
}
