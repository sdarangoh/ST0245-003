public int countClumps(int[] nums) {
  int clumps = 0;
  int k = 0;
  int count = 0;
  while(k < (nums.length - 1)){
    int i = k + 1;
    while((i < nums.length) && (nums[k] == nums[i])){
      if(count == 0){
        count = 1;
      }
      i++;
    }
    k = i;
    clumps = clumps + count;
    count = 0;
  }
  return clumps;
}
