public boolean modThree(int[] nums) {
  boolean modThree = false;
  for(int i = 0; i < nums.length - 2; i++){
    if(nums[i] % 2 == 0 && nums[i + 1] % 2 == 0 && nums[i + 2] % 2 == 0){ 
      modThree = true;}
    if(nums[i] % 2 != 0 && nums[i + 1] % 2 != 0 && nums[i + 2] % 2 != 0){
      modThree = true;
    }
  }
  return modThree;
}
