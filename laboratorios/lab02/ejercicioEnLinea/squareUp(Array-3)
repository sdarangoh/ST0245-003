public int[] squareUp(int n) {
  int[] square = new int[n*n];
  int k = n*n - 1;
  int l = 1;
  int m = n;
  for(int i = 0; i < n; i++){
    for(int j = k; j > k - n; j --){
      if(l > m){
        square[j] = 0;
        l++;
      }
      else{
        square[j] = l;
        l++;
      }
      if (l > n){
        l = 1;
      }
    }
    k = k - n;
    m = m - 1;
  }
  return square;
}
