public boolean canBalance(int[] nums) {
  boolean can = false;
  for(int k = 0; k < nums.length; k++){
    int sum1 = 0, sum2 = 0;
    for(int i = 0; i < k; i++){
      sum1 = sum1 + nums[i]; 
    }
    for(int j = k; j < nums.length; j++){ 
      sum2 = sum2 + nums[j];
    }
    if(sum1 == sum2){
      can = true;
    }
  }
  return can;
}
